<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="50" />
        <field name="inherit_id" ref="base.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('settings')]" position="inside">
                <field name="is_l10n_ro_record" invisible="1" />
            </xpath>
            <xpath
                expr="//div[@data-key='account']//field[@name='module_currency_rate_live']/../.."
                position="after"
            >
                <div
                    class="col-12 col-lg-6 o_setting_box"
                    attrs="{'invisible': [('is_l10n_ro_record','=',False)]}"
                >
                    <div class="o_setting_left_pane">
                        <field name="module_currency_rate_update_RO_BNR" />
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="module_currency_rate_update_RO_BNR" />
                        <span
                            class="fa fa-lg fa-building-o"
                            title="Values set here are company-specific."
                            role="img"
                            aria-label="Values set here are company-specific."
                            groups="base.group_multi_company"
                        />
                        <div
                            class="text-muted"
                            title="Will update daily from cron only if is checked Automatic Currency Rates (OCA) and you added a rate_provider"
                        >
                            Install BNR update rates
                        </div>
                    </div>
                </div>
            </xpath>
            <xpath expr="//div[hasclass('settings')]" position="inside">
                <div
                    class="app_settings_block"
                    data-string="Romania"
                    string="Romania"
                    data-key="l10n_ro_config"
                    groups="l10n_ro_config.group_ro_menus"
                >
                    <field
                        name="country_code"
                        invisible="1"
                        groups="l10n_ro_config.group_ro_menus"
                    />
                    <div
                        name="l10n_ro_config_container"
                        attrs="{'invisible': [('country_code', '!=', 'RO')]}"
                    >

                        <h2>Partners and Addresses</h2>
                        <div
                            class="row mt16 o_settings_container"
                            name="modules_contact_setting_container"
                        >
                            <div
                                class="col-12 col-lg-6 o_setting_box"
                                id="module_l10n_ro_partner_unique"
                            >
                                <div class="o_setting_left_pane">
                                    <field name="module_l10n_ro_partner_unique" />
                                </div>
                                <div class="o_setting_right_pane">
                                    <label for="module_l10n_ro_partner_unique" />
                                    <div
                                        class="text-muted"
                                    >Set up a rule for unique partners based on vat, nrc and
                                        company
                                    </div>
                                </div>
                            </div>
                            <div
                                class="col-12 col-lg-6 o_setting_box"
                                id="module_l10n_ro_partner_create_by_vat"
                            >
                                <div class="o_setting_left_pane">
                                    <field
                                        name="module_l10n_ro_partner_create_by_vat"
                                    />
                                </div>
                                <div class="o_setting_right_pane">
                                    <label for="module_l10n_ro_partner_create_by_vat" />
                                    <div
                                        class="text-muted"
                                    >Create partners based on the vat code from ANAF website.
                                    </div>
                                </div>
                            </div>
                            <div
                                class="col-12 col-lg-6 o_setting_box"
                                id="module_l10n_ro_fiscal_validation"
                            >
                                <div class="o_setting_left_pane">
                                    <field name="module_l10n_ro_fiscal_validation" />
                                </div>
                                <div class="o_setting_right_pane">
                                    <label for="module_l10n_ro_fiscal_validation" />
                                    <div
                                        class="text-muted"
                                    >Update partners data based on the vat code from ANAF website in
                                        cron job.
                                    </div>
                                </div>
                            </div>
                            <div
                                class="col-12 col-lg-6 o_setting_box"
                                id="module_l10n_ro_address_extended"
                            >
                                <div class="o_setting_left_pane">
                                    <field name="module_l10n_ro_address_extended" />
                                </div>
                                <div class="o_setting_right_pane">
                                    <label for="module_l10n_ro_address_extended" />
                                    <div
                                        class="text-muted"
                                    >Complete also staircase in extended address for partners.
                                    </div>
                                </div>
                            </div>
                            <div
                                class="col-12 col-lg-6 o_setting_box"
                                id="module_l10n_ro_city"
                            >
                                <div class="o_setting_left_pane">
                                    <field name="module_l10n_ro_city" />
                                </div>
                                <div class="o_setting_right_pane">
                                    <label for="module_l10n_ro_city" />
                                    <div
                                        class="text-muted"
                                    >Upload Romanian cities, so you can select them more easily.
                                    </div>
                                </div>
                            </div>
                            <div
                                class="col-12 col-lg-6 o_setting_box"
                                id="module_l10n_ro_siruta"
                            >
                                <div class="o_setting_left_pane">
                                    <field name="module_l10n_ro_siruta" />
                                </div>
                                <div class="o_setting_right_pane">
                                    <label for="module_l10n_ro_siruta" />
                                    <div
                                        class="text-muted"
                                    >Select also the commune and zone in partner addresses.
                                    </div>
                                </div>
                            </div>
                        </div>
