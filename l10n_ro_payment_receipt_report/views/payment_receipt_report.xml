<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- QWeb Reports -->

    <record id="l10n_ro_action_report_payment_receipt" model="ir.actions.report">
        <field name="name">Romania Payment Receipt</field>
        <field name="model">account.payment</field>
        <field name="report_type">qweb-pdf</field>
        <field
            name="report_name"
        >l10n_ro_payment_receipt_report.report_payment_receipt</field>
        <field
            name="report_file"
        >l10n_ro_payment_receipt_report.report_payment_receipt</field>
        <field
            name="paperformat_id"
            ref="l10n_ro_payment_receipt_report.paperformat_A5_landscape"
        />
        <field name="binding_model_id" ref="account.model_account_payment" />
        <field name="binding_type">report</field>
    </record>


</odoo>
