<?xml version="1.0"?>
<odoo>
    <record id="hr_contract_view_ro_form2" model="ir.ui.view">
        <field name="name">hr.contract.view.ro.form2</field>
        <field name="model">hr.contract</field>
        <field name="inherit_id" ref="l10n_ro_hr.hr_contract_view_ro_form"/>
        <field name="arch" type="xml">
            <field name="wage" position="after">
                <field name="net_wage"/>
            </field>
            <xpath expr="//field[@name='pensioneer']" position="after">
                <field name="programmer_or_handicaped"/>
            </xpath>
        </field>
    </record>
</odoo>
