<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id='view_stock_landed_cost_form' model='ir.ui.view'>
        <field name="model">stock.landed.cost</field>
        <field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_form" />
        <field name="arch" type="xml">
            <notebook position="before">
                <group>
                    <group string="Romania DVI">
                        <field name="l10n_ro_landed_type" />
                        <field name="l10n_ro_tax_id" />
                        <field name="l10n_ro_tax_value" />
                    </group>
                </group>
            </notebook>
        <xpath
                expr="//page[@name='additional_costs']/field/form/group/group"
                position="inside"
            >
            <field name="l10n_ro_tax_id" />
        </xpath>
        <xpath expr="//page[@name='additional_costs']/field/tree" position="inside">
            <field name="l10n_ro_tax_id" optional="hide" />
        </xpath>


        </field>
    </record>



</odoo>
