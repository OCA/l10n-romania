<odoo>
    <record id="stock_valuation_layer_form" model="ir.ui.view">
        <field name="model">stock.valuation.layer</field>
        <field name="inherit_id" ref="stock_account.stock_valuation_layer_form" />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Romania fields" name="l10n_ro" >
                    <group>
                        <field name="l10n_ro_valued_type" />
                        <field name="l10n_ro_draft_history" />
                        <field name="l10n_ro_bill_accounting_date" />
                    </group>
                </page>
            </notebook>
        </field>
    </record>
    <record id="stock_valuation_layer_tree" model="ir.ui.view">
        <field name="model">stock.valuation.layer</field>
        <field name="inherit_id" ref="stock_account.stock_valuation_layer_tree" />
        <field name="arch" type="xml">
            <field name="create_date" position="after">
                  <field name="l10n_ro_bill_accounting_date" optional="show" />
                  <field name="l10n_ro_valued_type" optional="show" />
            </field>
        </field>
    </record>

</odoo>
