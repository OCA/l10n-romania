<odoo>
    <record id="stock_valuation_layer_form" model="ir.ui.view">
        <field name="model">stock.valuation.layer</field>
        <field name="inherit_id" ref="stock_account.stock_valuation_layer_form" />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Romania fields" name="l10n_ro" attrs="{'invisible': [('is_l10n_ro_record', '=', False)]}">
                    <group>
                        <field name="is_l10n_ro_record" invisible="1" />
                        <field name="l10n_ro_valued_type" />
                        <field name="l10n_ro_account_id"/>
                        <field name="l10n_ro_invoice_line_id"/>
                        <field name="l10n_ro_invoice_id"/>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
