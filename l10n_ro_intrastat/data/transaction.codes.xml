        </record>
        <record id="intrastat_transaction_1_2" model="l10n_ro_intrastat.transaction">
            <field name="code">1.2</field>
            <field name="parent_id" ref="intrastat_transaction_1" />
            <field
                name="description"
            >Furnizarea pentru vânzare la vedere sau cu testare, pentru consignație sau prin intermediul unui agent
                comisionar
            </field>
        </record>
        <record id="intrastat_transaction_1_3" model="l10n_ro_intrastat.transaction">
            <field name="code">1.3</field>
