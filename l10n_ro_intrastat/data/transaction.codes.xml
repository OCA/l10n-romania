        <record id="intrastat_transaction_1" model="l10n_ro_intrastat.transaction">
            <field name="code">1</field>
            <field
                name="description"
            >Tranzacții care implică transferul real sau intenționat de proprietate de la rezidenți la nerezidenți în
                schimbul unei compensații financiare sau de altă natură (cu excepția tranzacțiilor enumerate la pct. 2,
                7 și 8)
            </field>
        </record>
        <record id="intrastat_transaction_1_1" model="l10n_ro_intrastat.transaction">
            <field name="code">1.1</field>
            <field name="parent_id" ref="intrastat_transaction_1" />
