<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="duunece_uom_uom_tree_view" model="ir.ui.view">
        <field name="name">duunece.uom.uom.tree</field>
        <field name="model">uom.uom</field>
        <field name="inherit_id" ref="uom.product_uom_tree_view"/>
        <field name="arch" type="xml">
        	<field name="category_id" position="after">
        		<field name="unece_code" />
        	</field>
        </field>
    </record>
    
    <record id="duunece_uom_uom_form_view" model="ir.ui.view">
        <field name="name">duunece.uom.uom.form</field>
        <field name="model">uom.uom</field>
        <field name="inherit_id" ref="uom.product_uom_form_view"/>
        <field name="arch" type="xml">
        	<field name="rounding" position="after">
        		<field name="unece_code" />
        	</field>            	
        </field>
    </record>
    
</odoo>